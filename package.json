    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "8.0.6",
    "concurrently": "^4.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "^1.19.2",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "1.7.2"
